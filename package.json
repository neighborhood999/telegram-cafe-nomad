{
  "name": "telegram-cafe-nomad",
  "version": "1.0.0",
  "description": "Helping you find cafe store by telegram bot.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint && ava --verbose",
    "watch": "npm test -- --watch",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test:cov": "nyc npm test"
  },
  "author": "Peng-Jie <dean.leehom@gmail.com> (https://github.com/neighborhood999)",
  "repository": {
    "type": "git",
    "url": "https://github.com/neighborhood999/telegram-cafe-nomad.git"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "files": "src/**/__tests__/*.spec.js",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "fetch-mock": "^5.12.2",
    "nyc": "^11.0.2",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "@google/maps": "^0.4.3",
    "dotenv": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "node-telegram-bot-api": "^0.28.0"
  },
  "license": "MIT"
}
