  "description": "Helping you find cafe store by telegram bot.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint && ava --verbose",
    "watch": "npm test -- --watch",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test:cov": "nyc npm test"
  },
  "author": "Peng-Jie <dean.leehom@gmail.com> (https://github.com/neighborhood999)",
  "repository": {
    "type": "git",
    "url": "https://github.com/neighborhood999/telegram-cafe-nomad.git"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "files": "src/**/__tests__/*.spec.js",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "fetch-mock": "^5.11.0",
    "nyc": "^10.2.2",
    "prettier": "^1.3.1"
  },
  "dependencies": {
    "@google/maps": "^0.3.1",
    "dotenv": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "node-telegram-bot-api": "^0.27.0"
  },
  "license": "MIT"
}
